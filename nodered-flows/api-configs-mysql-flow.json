    {
        "id": "api-configs-mysql-save",
        "type": "http in",
        "z": "api-configs-mysql-flow",
        "name": "保存API配置",
        "url": "/api/api-configs/save",
        "method": "post",
        "upload": false,
        "swaggerDoc": "",
        "x": 120,
